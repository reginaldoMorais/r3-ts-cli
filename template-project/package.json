{
  "name": "{{APP_TITLE}}",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -Rf dist coverage lib",
    "analyse": "NODE_ENV=analyse webpack --config webpack.config.js -p",
    "build": "npm run clean && NODE_ENV=${NODE_ENV:=production} webpack --config ./webpack.config.js --colors",
    "start": "PORT=${PORT:=8080} NODE_ENV=${NODE_ENV:=production} npm run build && npm run server",
    "start:dev": "PORT=${PORT:=8081} NODE_ENV=${NODE_ENV:=development} webpack-dev-server --progress --colors --inline --open --hot",
    "server": "PORT=${PORT:=8080} NODE_ENV=${NODE_ENV:=production} ts-node src/server",
    "test": "jest",
    "test:w": "jest --watchAll",
    "test:v": "jest --verbose",
    "test:c": "jest --coverage",
    "test:d": "jest --debug",
    "lint:e": "eslint src/**/*.tsx",
    "lint:s": "sass-lint --verbose",
    "type-check": "tsc --skipLibCheck --noEmit",
    "type-check:w": "npm run type-check -- --watch",
    "ls": "node commands-ls.js",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "author": "reginaldo.cmorais",
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
